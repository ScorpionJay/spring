<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.repository.mybatis.ProductRepository2">

    <select id="fingByName2" resultType="com.entity.jpa.Product">
        select * from product;
    </select>


    <insert id="save">
        INSERT INTO product (name,rate) VALUES (#{name},#{rate})
    </insert>

</mapper>
